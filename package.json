{
  "name": "backend",
  "version": "0.0.1",
  "description": "Medusa v2 backend",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "keywords": [
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "preinstall": "corepack enable && corepack prepare yarn@3.2.3 --activate",
    "dev": "medusa develop",
    "predeploy": "medusa db:migrate",
    "build": "medusa build",
    "start": "medusa start --host 0.0.0.0 --port $PORT",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "create-admin": "medusa exec ./src/scripts/create-admin.ts",
    "create-publishable-key": "medusa exec ./src/scripts/create-publishable-key.ts",
    "db:migrate": "medusa db:migrate",
    "db:rollback": "medusa db:rollback",
    "db:sync-links": "medusa db:sync-links",
    "db:init": "yarn db:migrate && yarn db:sync-links && yarn seed",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --runInBand --forceExit",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --runInBand --forceExit"
  },
  "dependencies": {
    "@medusajs/admin-sdk": "2.12.5",
    "@medusajs/cli": "2.12.5",
    "@medusajs/framework": "2.12.5",
    "@medusajs/index": "2.12.5",
    "@medusajs/medusa": "2.12.5",
    "@react-email/components": "1.0.4",
    "pg": "^8.13.0",
    "resend": "^6.7.0"
  },
  "devDependencies": {
    "@medusajs/test-utils": "2.12.5",
    "@swc/core": "^1.7.28",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.12.11",
    "@types/prop-types": "^15.7.15",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "jest": "^29.7.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "vite": "^5.4.14",
    "yalc": "^1.0.0-pre.53"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "yarn@3.2.3"
}
